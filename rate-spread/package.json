{
  "name": "rate-spread-tool",
  "version": "0.0.0",
  "description": "Tool to generate the spread between an Annual Percentage Rate (APR) and a survey-based estimate of APRs currently offered on prime mortgage loans.",
  "main": "index.js",
  "engines": {
    "node": ">=8.x"
  },
  "scripts": {
    "test": "jest --coverage",
    "clearDist": "rimraf ./dist && mkdirp ./dist ./dist/js ./dist/css ./dist/fonts ./dist/img",
    "js": "webpack --progress",
    "js:dev": "webpack --config webpack.dev.js --progress",
    "sass": "node-sass --quiet --output-style=compressed --source-map=./dist/css/ ./src/sass/app.scss ./dist/css/app.min.css && cp node_modules/uswds/src/fonts/sourcesanspro-* ./dist/fonts",
    "html": "html-minifier --collapse-whitespace ./src/index.html -o ./dist/index.html",
    "img": "cp ./src/img/min/* ./dist/img && cp -r ./node_modules/uswds/dist/img/* ./dist/img",
    "postinstall": "yarn run clearDist && yarn run build",
    "watch:js": "yarn run js:dev -- --watch",
    "watch:sass": "node-sass --watch --quiet --output-style=compressed --source-map=./dist/css/ ./src/sass/app.scss ./dist/css/app.min.css",
    "watch:tests": "jest --watchAll",
    "watch": "yarn run watch:js & yarn run watch:sass",
    "build": "yarn run js && yarn run sass && yarn run html && yarn run img"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfpb/hmda-platform-tools.git"
  },
  "keywords": [
    "HMDA"
  ],
  "author": "Consumer Financial Protection Bureau",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/cfpb/hmda-platform-tools/issues"
  },
  "homepage": "https://github.com/cfpb/hmda-platform-tools#readme",
  "devDependencies": {
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "html-minifier": "3.5.5",
    "imagemin": "5.3.1",
    "imagemin-cli": "3.0.0",
    "jest-cli": "21.2.1",
    "mkdirp": "0.5.1",
    "node-sass": "4.5.3",
    "react-addons-test-utils": "15.6.2",
    "rimraf": "2.6.2",
    "webpack": "3.7.1",
    "webpack-merge": "4.1.0"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/Wrapper.js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/redux-mock-store",
      "<rootDir>/node_modules/redux-thunk",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/__tests__/Wrapper.js"
    ]
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "detect-browser": "2.0.0",
    "isomorphic-fetch": "2.2.1",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "uswds": "1.4.1"
  }
}
